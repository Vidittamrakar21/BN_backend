{
    "builds":[
        {
            "src":"./index.js",
            "use":"@vercel/node"
        }
    ],
    "routes":[
        {
            "src":"/(.*)",
            "dest":"index.js"
        },
        {
            "src":"/api/(.*)",
            "dest":"index.js"
        },
        {
            "src":"/check",
            "dest":"index.js"
        }
        
    ]
}
